{
  "name": "@zinger/aws",
  "version": "1.0.0",
  "private": true,
  "bin": {
    "zinger-ops": "bin/zinger.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig-build.json",
    "cdk": "cdk --profile zinger --app 'yarn run-ts bin/zinger-prod.ts'",
    "cdk:deploy": "make -j seq",
    "cdk:destroy": "make -j destroy",
    "cdk:sandbox": "cdk --profile zinger-sandbox --app 'yarn run-ts bin/zinger-sandbox.ts'",
    "cdk:sandbox:deploy": "make CDK=cdk:sandbox -j seq",
    "cdk:staging": "cdk --profile zinger-staging --app 'yarn run-ts bin/zinger-staging.ts'",
    "cdk:staging:deploy": "make CDK=cdk:staging -j seq",
    "clean": "rimraf *.{js,d.ts,tsbuildinfo} bin/*.{js,d.ts} cdk.out lib/*.{js,d.ts}",
    "ec2:bastion:start": "yarn run-ts scripts/ec2-bastion-start.ts",
    "ec2:bastion:stop": "yarn run-ts scripts/ec2-bastion-stop.ts",
    "ec2:key-pair:delete": "yarn run-ts scripts/ec2-key-pair-delete.ts",
    "ecr:repository:delete": "yarn run-ts scripts/ecr-repository-delete.ts",
    "ecs:create:queue": "yarn run-ts scripts/ecs-service-create.ts --task queue",
    "ecs:create:service": "yarn run-ts scripts/ecs-service-create.ts --task service",
    "ecs:delete:queue": "yarn run-ts scripts/ecs-service-delete.ts --task queue",
    "ecs:delete:service": "yarn run-ts scripts/ecs-service-delete.ts --task service",
    "ecs:register": "yarn run-ts scripts/ecs-register-task-definition.ts",
    "ecs:run:batch": "yarn run-ts scripts/ecs-batch-run.ts",
    "ecs:run:migration": "yarn run-ts scripts/ecs-migration-run.ts",
    "ecs:update:queue": "yarn run-ts scripts/ecs-service-update.ts --task queue",
    "ecs:update:service": "yarn run-ts scripts/ecs-service-update.ts --task service",
    "events:delete": "yarn run-ts scripts/events-delete.ts",
    "events:register:rule": "yarn run-ts scripts/events-put-rule.ts",
    "events:register:target": "yarn run-ts scripts/events-put-targets.ts",
    "get-ecr-host": "yarn run-ts scripts/get-ecr-host.ts",
    "iam:user:create": "yarn run-ts scripts/iam-user-create.ts",
    "iam:user:delete": "yarn run-ts scripts/iam-user-delete.ts",
    "push-docker-images:prod": "scripts/push-docker-images.sh zinger prod",
    "push-docker-images:sandbox": "scripts/push-docker-images.sh zinger-sandbox sandbox",
    "push-docker-images:staging": "scripts/push-docker-images.sh zinger-staging staging",
    "rds:delete": "yarn run-ts scripts/rds-delete.ts",
    "route53:hosted-zone:create": "yarn run-ts scripts/route53-hosted-zone-create.ts",
    "route53:hosted-zone:delete": "yarn run-ts scripts/route53-hosted-zone-delete.ts",
    "run-ts": "ts-node -r tsconfig-paths/register",
    "s3:delete": "yarn run-ts scripts/s3-delete.ts",
    "ssm:delete:prod": "run-s ssm:fetch ssm:delete:prod:run",
    "ssm:delete:prod:run": "yarn run-ts scripts/ssm-delete.ts -p zinger",
    "ssm:delete:sandbox": "run-s ssm:fetch ssm:delete:sandbox:run",
    "ssm:delete:sandbox:run": "yarn run-ts scripts/ssm-delete.ts -p zinger-sandbox",
    "ssm:delete:staging": "run-s ssm:fetch ssm:delete:staging:run",
    "ssm:delete:staging:run": "yarn run-ts scripts/ssm-delete.ts -p zinger-staging",
    "ssm:fetch": "scripts/ssm-get-parameters.sh",
    "ssm:register:prod": "run-s ssm:fetch ssm:register:prod:run",
    "ssm:register:prod:run": "yarn run-ts scripts/ssm-register.ts -p zinger",
    "ssm:register:sandbox": "run-s ssm:fetch ssm:register:sandbox:run",
    "ssm:register:sandbox:run": "yarn run-ts scripts/ssm-register.ts -p zinger-sandbox",
    "ssm:register:staging": "run-s ssm:fetch ssm:register:staging:run",
    "ssm:register:staging:run": "yarn run-ts scripts/ssm-register.ts -p zinger-staging"
  },
  "dependencies": {
    "@types/ini": "^4.0.0",
    "@types/prompts": "^2.0.12",
    "@zinger/helpers": "^1.0.0",
    "aws-cdk-lib": "^2.33.0",
    "aws-sdk": "^2.1178.0",
    "commander": "^12.0.0",
    "constructs": "^10.1.51",
    "luxon": "^3.0.1",
    "prompts": "^2.4.2",
    "source-map-support": "^0.5.20",
    "tsconfig-paths": "^4.0.0"
  },
  "devDependencies": {
    "@types/luxon": "^3.0.0",
    "@types/node": "^20.0.0",
    "aws-cdk": "^2.33.0",
    "change-case": "^5.0.0",
    "npm-run-all2": "^5.0.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.4.0"
  }
}
